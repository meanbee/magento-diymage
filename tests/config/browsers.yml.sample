---
browsers:
    chrome: &chrome
        browser: '*chrome'
        host: 127.0.0.1
        port: 4444
        doNotKillBrowsers: false
        browserTimeoutPeriod: 40000
    default: *chrome
applications:
    magento: &magento
        frontendUrl: http://www.localhost.com/magento/
        adminUrl: http://www.localhost.com/magento/admin/
        adminLogin: admin
        adminPassword: admin
    default: *magento
